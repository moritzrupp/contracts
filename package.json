{
  "name": "contracts",
  "description": "Contracts helps you to stay on track with all your contracts and subscriptions!",
  "private": true,
  "keywords": [
    "contract",
    "subscription",
    "manage"
  ],
  "version": "0.0.2",
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Moritz Rupp",
    "email": "moritz.rupp@gmail.com"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/moritzrupp/contracts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moritzrupp/contracts.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "commit": "cz",
    "lint": "next lint",
    "lint:fix": "next lint . --fix --ext .js,.jsx,.ts,.tsx",
    "semantic-release": "semantic-release"
  },
  "auto-changelog": {
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false,
    "package": false
  },
  "dependencies": {
    "next": "^12.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.1",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.39",
    "auto-changelog": "^2.4.0",
    "autoprefixer": "^10.4.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "semantic-release": "^17.4.7",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
