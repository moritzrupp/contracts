{
  "name": "contracts",
  "description": "Contracts helps you to stay on track with all your contracts and subscriptions!",
  "keywords": [
    "contract",
    "subscription",
    "manage"
  ],
  "version": "0.0.1",
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Moritz Rupp",
    "email": "moritz.rupp@gmail.com"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/moritzrupp/contracts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moritzrupp/contracts.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "version": "auto-changelog && git add CHANGELOG.md",
    "changelog": "auto-changelog",
    "changelog-debug": "auto-changelog --template json --output changelog-data.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx"
  },
  "auto-changelog": {
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false,
    "package": true
  },
  "dependencies": {
    "next": "10.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^10.0.3",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "auto-changelog": "^2.2.1",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-nextjs": "^1.0.6",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "postcss": "^8.2.1",
    "prettier": "1.19.1",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
